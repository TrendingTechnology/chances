{
  "name": "chances",
  "private": true,
  "description": "Chances",
  "version": "1.0.0",
  "author": "Chance Strickland <chance.strickland@gmail.com>",
  "scripts": {
    "dev": "gatsby develop -p 6060",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "start": "yarn dev",
    "build": "rm -rf public && rm -rf .cache && gatsby build"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "css-mqpacker": "^7.0.0",
    "fontfaceobserver": "^2.1.0",
    "formik": "^1.5.1",
    "gatsby": "^2.1.38",
    "gatsby-plugin-feed": "^2.0.15",
    "gatsby-plugin-google-analytics": "^2.0.17",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-postcss": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sharp": "^2.0.29",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-transformer-remark": "^2.3.5",
    "gatsby-transformer-sharp": "^2.1.17",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "lodash": "^4.17.11",
    "postcss": "^7.0.14",
    "postcss-font-smoothing": "^0.1.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "prismjs": "^1.15.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^2.6.1",
    "striptags": "^3.1.1",
    "what-input": "^5.1.4"
  },
  "devDependencies": {
    "babel-eslint": "^9.x",
    "eslint": "5.x",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "gh-pages": "^2.0.1",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-property-sort-order-smacss": "^4.0.2",
    "stylelint-config-recommended": "^2.1.0"
  },
  "browserslist": [
    "> 3%",
    "edge 17",
    "not IE 11",
    "safari 12",
    "firefox 58"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  },
  "license": "MIT"
}

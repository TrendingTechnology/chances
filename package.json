{
  "name": "chances",
  "private": true,
  "description": "Chances",
  "version": "1.0.0",
  "author": "Chance Strickland <chance.strickland@gmail.com>",
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --write '{gatsby-*.js,src/**/*.{js,jsx,json,css}}'",
    "start": "yarn dev",
    "build": "rm -rf public && rm -rf .cache && gatsby build",
    "deploy": "yarn build && cp now.json public/ && cd public && now alias $(now) chances.tech",
    "dry": "yarn build && cp now.json public/ && cd public && now"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "css-mqpacker": "^7.0.0",
    "fontfaceobserver": "^2.1.0",
    "gatsby": "^2.0.76",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-i18n": "^0.4.2",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-postcss": "^2.0.2",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.16",
    "gatsby-remark-autolink-headers": "^2.0.12",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.17",
    "gatsby-transformer-sharp": "^2.1.9",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "postcss-font-smoothing": "^0.1.0",
    "postcss-nested": "^4.1.1",
    "postcss-preset-env": "^6.5.0",
    "prismjs": "^1.15.0",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "babel-eslint": "9.x",
    "eslint": "5.x",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-react-app": "^3.0.6",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "gh-pages": "^1.2.0",
    "prettier": "^1.14.2",
    "stylelint": "^9.10.1",
    "stylelint-config-property-sort-order-smacss": "^4.0.2",
    "stylelint-config-recommended": "^2.1.0"
  },
  "browserslist": [
    "> 3%",
    "edge 17",
    "not IE 11",
    "safari 12",
    "firefox 58"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{gatsby-*.js,src/**/*.{js,jsx,json,css}}": [
      "yarn format",
      "git add"
    ]
  },
  "license": "MIT"
}
